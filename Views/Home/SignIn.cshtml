@model List<Emp.Models.AttendanceView>
@{
}

<h1>SignIn</h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Time In</th>
            <th>Time Out</th>
            <th>Working Hour</th>
            <th>Department</th>
            <th>Phone</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.Id</td>
                <td>@employee.Name</td>
                <td>@employee.TimeIn</td>
                <td>@employee.TimeOut</td>
                <th>@employee.PhoneNumber</th>
                <td><a href="Employees/view/@employee.Id">View</a></td>
            </tr>
        }
    </tbody>
</table>